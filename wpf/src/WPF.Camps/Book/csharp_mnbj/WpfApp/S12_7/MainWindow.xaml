<Window x:Class="S12_7.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:S12_7"
        mc:Ignorable="d"
        Title="布局" Height="450" Width="800">
    <Grid>
        <StackPanel Width="300">
            <Button Margin="5,5,5,5" Content="① 自定义Panel子类" x:Name="btnMyPanel" Click="btnMyPanel_Click"/>
             
            <Button Margin="5,5,5,5" Content="⑤ 绝对定位（Canvas）" x:Name="btnCanvas" Click="btnCanvas_Click"/> 
            <Button Margin="5,5,5,5" Content="⑦ Z顺序（ZIndex）" x:Name="btnZIndex" Click="btnZIndex_Click"/>


        </StackPanel>
    </Grid>
</Window>
