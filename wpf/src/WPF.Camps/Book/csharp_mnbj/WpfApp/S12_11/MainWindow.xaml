<Window x:Class="S12_11.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:S12_11"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid> 
        <StackPanel Width="300">
            <Button Margin="5,5,5,5" Content="① 简单动画、演示面板 Storyboard" x:Name="btnStoryboard" Click="btnStoryboard_Click"/>
            <Button Margin="5,5,5,5" Content="② 关键帧动画" x:Name="btnDongHua" Click="btnDongHua_Click"/>

        </StackPanel>
    </Grid>
</Window>
